  "type": "library",
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.8",
    "rector/rector": "^0.13.5",
    "phpstan/phpstan": "^1.7"
  },
  "license": "AGPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "Subugoe\\OaiModel\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Ingo Pfennigstorf",
      "email": "i.pfennigstorf@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0 || ^8.1",
    "jms/serializer": "^3.17",
    "subugoe/iiif-model": "^0.2.4",
